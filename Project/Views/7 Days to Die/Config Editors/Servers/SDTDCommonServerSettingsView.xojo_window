	#tag Method, Flags = &h0
		Sub RefreshUI()
		  Var Profile As SDTD.ServerProfile = Self.Profile
		  
		  If Self.SettingUp Or Profile Is Nil Then
		    Return
		  End If
		  
		  Self.mSettingUp = True
		  
		  Self.ServerNameField.Text = Profile.Name
		  Self.ServerNicknameField.Text = Profile.Nickname
		  Self.DescriptionArea.Text = Profile.Description
		  Self.MapMenu.SetText(Profile.Map)
		  Self.SeedField.Text = Profile.MapSeed
		  Self.WorldSizeMenu.Text = Profile.MapSize.ToString(Locale.Current, "0")
		  
		  If Profile.Password Is Nil Then
		    Self.ServerPasswordSwitch.Value(False) = False
		    Self.ServerPasswordField.Text = ""
		  Else
		    Self.ServerPasswordSwitch.Value(False) = True
		    Self.ServerPasswordField.Text = Profile.Password.StringValue
		  End If
		  
		  If Profile.Port Is Nil Then
		    Self.PortSwitch.Value(False) = False
		    Self.PortField.Text = ""
		  Else
		    Self.PortSwitch.Value(False) = True
		    Self.PortField.Text = Profile.Port.IntegerValue.ToString(Locale.Current, "0")
		  End If
		  
		  If Profile.TelnetPort Is Nil Then
		    Self.TelnetPortSwitch.Value(False) = False
		    Self.TelnetPortField.Text = ""
		  Else
		    Self.TelnetPortSwitch.Value(False) = True
		    Self.TelnetPortField.Text = Profile.TelnetPort.IntegerValue.ToString(Locale.Current, "0")
		  End If
		  
		  Var ShowMapSeed As Boolean = Self.MapMenu.Text = "RWG"
		  Var NeedsLayoutUpdate As Boolean = (ShowMapSeed = True And Self.SeedField.Visible = False) Or (ShowMapSeed = False And Self.SeedField.Visible = True)
		  If NeedsLayoutUpdate Then
		    Var Labels() As DesktopLabel = Array(Self.ServerNameLabel, Self.ServerNicknameLabel, Self.ColorLabel, Self.ConfigSetLabel, Self.MapLabel, Self.DescriptionLabel, Self.ServerPasswordLabel, Self.PortLabel, Self.TelnetPortLabel)
		    If ShowMapSeed Then
		      Labels.Add(Self.SeedLabel)
		      Labels.Add(Self.WorldSizeLabel)
		    End If
		    BeaconUI.SizeToFit(Labels)
		    
		    Var FieldsLeft As Integer = Self.ServerNameLabel.Right + 12
		    Var FieldsWidth As Integer = Self.Width - (20 + FieldsLeft)
		    
		    Self.ServerNameField.Left = FieldsLeft
		    Self.ServerNameField.Width = FieldsWidth
		    Self.ServerNicknameField.Left = FieldsLeft
		    Self.ServerNicknameField.Width = FieldsWidth
		    Self.ColorChooser.Left = FieldsLeft
		    Self.ConfigSetField.Left = FieldsLeft
		    Self.ConfigSetField.Width = FieldsWidth - (Self.ConfigSetChooseButton.Width + 12)
		    Self.MapMenu.Left = FieldsLeft
		    Self.SeedField.Left = FieldsLeft
		    Self.SeedGenerateButton.Left = Self.SeedField.Right + 12
		    Self.WorldSizeMenu.Left = FieldsLeft
		    Self.DescriptionArea.Left = FieldsLeft
		    Self.DescriptionArea.Width = FieldsWidth
		    Self.ServerPasswordSwitch.Left = FieldsLeft
		    Self.ServerPasswordField.Left = Self.ServerPasswordSwitch.Right + 12
		    Self.ServerPasswordField.Width = FieldsWidth - (Self.ServerPasswordSwitch.Width + 12)
		    Self.PortSwitch.Left = FieldsLeft
		    Self.PortField.Left = Self.PortSwitch.Right + 12
		    Self.PortField.Width = FieldsWidth - (Self.PortSwitch.Width + 12)
		    Self.TelnetPortSwitch.Left = FieldsLeft
		    Self.TelnetPortField.Left = Self.TelnetPortSwitch.Right + 12
		    Self.TelnetPortField.Width = FieldsWidth - (Self.TelnetPortSwitch.Width + 12)
		    
		    Self.SeedLabel.Visible = ShowMapSeed
		    Self.SeedField.Visible = ShowMapSeed
		    Self.SeedGenerateButton.Visible = ShowMapSeed
		    Self.WorldSizeLabel.Visible = ShowMapSeed
		    Self.WorldSizeMenu.Visible = ShowMapSeed
		    
		    If ShowMapSeed Then
		      Self.DescriptionArea.Top = Self.WorldSizeMenu.Bottom + 12
		    Else
		      Self.DescriptionArea.Top = Self.MapMenu.Bottom + 12
		    End If
		    
		    Self.DescriptionLabel.Top = Self.DescriptionArea.Top
		    Self.ServerPasswordLabel.Top = Self.DescriptionArea.Bottom + 12
		    Self.ServerPasswordSwitch.Top = Self.ServerPasswordLabel.Top
		    Self.ServerPasswordField.Top = Self.ServerPasswordLabel.Top
		    Self.PortLabel.Top = Self.ServerPasswordLabel.Bottom + 12
		    Self.PortSwitch.Top = Self.PortLabel.Top
		    Self.PortField.Top = Self.PortLabel.Top
		    Self.TelnetPortLabel.Top = Self.PortLabel.Bottom + 12
		    Self.TelnetPortSwitch.Top = Self.TelnetPortLabel.Top
		    Self.TelnetPortField.Top = Self.TelnetPortLabel.Top
		  End If
		  
		  Self.ColorChooser.SelectedColor = Profile.ProfileColor
		  
		  Self.UpdateConfigSetUI()
		  
		  Self.mSettingUp = False
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateConfigSetUI()
		  Var Project As SDTD.Project = Self.Project
		  
		  If Project.ConfigSetCount > 1 Then
		    Var Sets() As Beacon.ConfigSet = Beacon.ConfigSetState.FilterSets(Self.Profile.ConfigSetStates, Project.ConfigSets)
		    
		    Var EnabledSets() As String
		    For Each Set As Beacon.ConfigSet In Sets
		      EnabledSets.Add(Set.Name)
		    Next
		    
		    Self.ConfigSetField.Text = EnabledSets.EnglishOxfordList()
		    Self.ConfigSetChooseButton.Enabled = True
		  Else
		    Self.ConfigSetField.Text = Beacon.ConfigSet.BaseConfigSet.Name
		    Self.ConfigSetChooseButton.Enabled = False
		  End If
		End Sub
	#tag EndMethod


