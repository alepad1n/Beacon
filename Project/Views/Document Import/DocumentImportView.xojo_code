#tag Class
Protected Class DocumentImportView
Inherits DesktopContainer
	#tag Method, Flags = &h1
		Protected Sub Dismiss()
		  RaiseEvent ShouldDismiss
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Sub Finish(Projects() As Beacon.Project)
		  RaiseEvent ProjectsImported(Projects)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Import(File As FolderItem)
		  RaiseEvent ImportFile(File)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub PullValuesFromProject(Project As Beacon.Project)
		  RaiseEvent PullValuesFromProject(Project)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Reset()
		  RaiseEvent Reset()
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetOtherProjects(Projects() As Beacon.Project)
		  RaiseEvent SetOtherProjects(Projects)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Sub SetPageHeight(Height As Integer)
		  If Height <> Self.Height Then
		    RaiseEvent ShouldResize(Height)
		    Self.Height = Height
		  End If
		End Sub
	#tag EndMethod


	#tag Hook, Flags = &h0
		Event ImportFile(File As FolderItem)
	#tag EndHook

	#tag Hook, Flags = &h0
