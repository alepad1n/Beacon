		Sub TextChanged(index as Integer)
		  If Self.mSettingUp Or IsNumeric(Me.Text) = False Then
		    Return
		  End If
		  
		  Var Multiplier As Double = CDbl(Me.Text)
		  RaiseEvent MultiplierUpdated(Index, Multiplier)
		End Sub
