#tag WindowCode
	#tag Event
		Sub Opening()
		  Self.MinimumWidth = 717
		End Sub
	#tag EndEvent

	#tag Event
		Sub SetupUI()
		  Var Config As Ark.Configs.StatLimits = Self.Config(False)
		  For Stat As Integer = Ark.Configs.StatLimits.StatGenericQuality To Ark.Configs.StatLimits.StatHyperthermal
		    Var Value As NullableDouble = Config.Value(Stat)
		    If Value = Nil Then
		      Self.StatCheckboxes(Stat).Value = False
		      Self.StatFields(Stat).Text = ""
		    Else
		      Self.StatCheckboxes(Stat).Value = True
		      Var DoubleValue As Double = Value
		      Self.StatFields(Stat).Text = DoubleValue.PrettyText
		    End If
		  Next
		  
		  Self.UpdateList()
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Shared Function BuildStatDict(InitialValueConstant As Double, StateModifierScale As Double, RandomizerRangeMultiplier As Double) As Dictionary
		  Var Dict As New Dictionary
		  Dict.Value("IVC") = InitialValueConstant
		  Dict.Value("SMS") = StateModifierScale
		  Dict.Value("RRM") = RandomizerRangeMultiplier
		  Return Dict
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Shared Function ColumnIsPercentage(Column As Integer) As Boolean
		  Select Case Column
		  Case ColumnDamage, ColumnGeneric, ColumnAmmo
		    Return True
		  End Select
		  
		  Return False
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Shared Function ColumnToStat(Column As Integer) As Integer
		  Select Case Column
		  Case ColumnAmmo
		    Return Ark.Configs.StatLimits.StatWeaponAmmo
		  Case ColumnArmor
		    Return Ark.Configs.StatLimits.StatArmor
		  Case ColumnDamage
		    Return Ark.Configs.StatLimits.StatWeaponDamage
		  Case ColumnDurability
		    Return Ark.Configs.StatLimits.StatMaxDurability
		  Case ColumnGeneric
		    Return Ark.Configs.StatLimits.StatGenericQuality
		  Case ColumnHyper
		    Return Ark.Configs.StatLimits.StatHyperthermal
		  Case ColumnHypo
		    Return Ark.Configs.StatLimits.StatHypothermal
		  Case ColumnWeight
		    Return Ark.Configs.StatLimits.StatWeight
		  End Select
		End Function
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Function Config(ForWriting As Boolean) As Ark.Configs.StatLimits
		  Return Ark.Configs.StatLimits(Super.Config(ForWriting))
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function InternalName() As String
		  Return Ark.Configs.NameStatLimits
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Shared Function StatToColumn(Stat As Integer) As Integer
		  Select Case Stat
		  Case Ark.Configs.StatLimits.StatArmor
		    Return ColumnArmor
		  Case Ark.Configs.StatLimits.StatGenericQuality
		    Return ColumnGeneric
		  Case Ark.Configs.StatLimits.StatHyperthermal
		    Return ColumnHyper
		  Case Ark.Configs.StatLimits.StatHypothermal
		    Return ColumnHypo
		  Case Ark.Configs.StatLimits.StatMaxDurability
		    Return ColumnDurability
		  Case Ark.Configs.StatLimits.StatWeaponAmmo
		    Return ColumnAmmo
		  Case Ark.Configs.StatLimits.StatWeaponDamage
		    Return ColumnDamage
		  Case Ark.Configs.StatLimits.StatWeight
		    Return ColumnWeight
		  End Select
		End Function
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Sub UpdateList()
		  If Self.mPreviewItemStats.LastIndex = -1 Then
		    Var Dict As Dictionary
		    Var DataSource As Ark.DataSource = Ark.DataSource.Pool.Get(False)
		    
		    Dict = New Dictionary
		    Dict.Value("Engram") = DataSource.GetEngramByUUID("fbe652a8-62f0-4daf-af89-a2d501b4f3f8") // Bow
		    Dict.Value(Ark.Configs.StatLimits.StatMaxDurability) = Self.BuildStatDict(50.0, 0.001, 0.25)
		    Dict.Value(Ark.Configs.StatLimits.StatWeaponDamage) = Self.BuildStatDict(100.0, 0.001, 0.1)
		    Dict.Value(Ark.Configs.StatLimits.StatWeaponAmmo) = Self.BuildStatDict(0.0, 1.0, 0.0)
		    Self.mPreviewItemStats.Add(Dict)
		    
		    Dict = New Dictionary
