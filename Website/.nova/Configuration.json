{
  "apexskier.eslint.config.eslintConfigPath" : ".eslintrc.yml",
  "apexskier.eslint.config.eslintPath" : "node_modules\/.bin\/eslint",
  "apexskier.eslint.config.eslintRulesDirs" : [

  ],
  "index.enabled" : true,
  "index.ignored_file_patterns" : [
    "www\/assets\/scripts",
    "www\/assets\/css",
    "node_modules"
  ],
  "index.symbolicate" : true,
  "index.use_scm_ignored_files" : true,
  "php.interpreter" : "\/opt\/homebrew\/bin\/php",
  "php.validate" : "onChange",
  "VineCode.Sass.cssStyle" : "Compressed",
  "VineCode.Sass.errorCss" : "No",
  "VineCode.Sass.execPath" : "\/opt\/homebrew\/bin\/sass",
  "VineCode.Sass.sourceMap" : "None",
  "workspace.art_style" : 1,
  "workspace.color" : 11,
  "workspace.name" : "Beacon Local",
  "workspace.preview_root" : "www",
  "workspace.preview_type" : "custom",
  "workspace.preview_url" : "https:\/\/local.usebeacon.app"
}
